  <lb/>dearth of <del facs="#img_233_z027" rend="multi-stroke _HMp" hand="#HM" change="#StEb">all</del> <add facs="#img_233_z028" rend="no-caret _HMp" place="above" hand="#HM" change="#StEb">exact</add> knowledge as to his <del facs="#img_233_z030" rend="multi-stroke _HMp" hand="#HM" change="#StEb">real</del>
  <lb/><subst facs="#img_233_z032"><del facs="#img_233_z032" rend="overwritten _ink1" type="superimposed" hand="#HM">anticedents</del><add facs="#img_233_z032" rend="superimposed _HMp" type="superimposed" hand="#HM" change="#StEb">antecedents</add></subst>, opened <metamark facs="#img_233_z050" place="inline" function="caret" change="#StEb" rend="caret _ink1">^</metamark> <add facs="#img_233_z037" rend="caret _ink1" place="above" hand="#HM" change="#StEb">to the invidious</add> <del facs="#img_233_z035" rend="single-stroke _ink1" hand="#HM" change="#StEb">a somewhat</del> <del facs="#img_233_z051" rend="single-stroke _ink1" hand="#HM" change="#StEb"><add facs="#img_233_z051" rend="no-caret _ink1" place="inline" hand="#HM" change="#StEb">wide</add></del>
  <lb/><del facs="#img_233_z033" rend="single-stroke _ink1" hand="#HM" change="#StEb">mysterious</del> <del facs="#img_233_z055" rend="single-stroke _ink1" hand="#HM" change="#StEb">field for invidious</del>
  <lb/><del facs="#img_233_z038" rend="single-stroke _ink1" hand="#HM" change="#StEb">doubt and surmise</del> <add facs="#img_233_z053" rend="no-caret _ink1" place="inline" hand="#HM" change="#StEb"> <metamark facs="#img_233_z054" place="inline" function="insertion-device" change="#StEb" rend="#insertion-device _ink1">/</metamark> a wide field,
  <lb/>however vague, for <restore facs="#img_233_z040" change="#StEb"><del facs="#img_233_z040" rend="single-stroke _HMp" hand="#HM"><hi rend="restoration">unfavorable</hi></del></restore> <add facs="#img_233_z041" rend="no-caret _HMp" place="above" hand="#HM" change="#StEb">invidious</add> <del facs="#img_233_z039" rend="multi-stroke _HMp" hand="#HM" change="#StEb">doubt</del>
