  <lb/>present and to come of <del facs="#img_163_z026" rend="multi-stroke" hand="#ink1r" change="#StBa">a</del> <metamark facs="#img_163_z027" place="inline" function="caret" change="#StBa" rend="#ink1r">^</metamark> <add facs="#img_163_z028" rend="caret" place="above" hand="#HMp" change="#StBa"><subst facs="#img_163_z028"><del facs="#img_163_z028" rend="overwritten" type="superimposed" hand="#HMp">the</del><add facs="#img_163_z028" rend="superimposed" type="superimposed" hand="#ink1r" change="#StBa">the</add></subst></add> convulsed
  <lb/><subst facs="#img_163_z046"><del facs="#img_163_z046" rend="overwritten" type="superimposed" hand="#HMp">continent</del><add facs="#img_163_z046" rend="superimposed" type="superimposed" hand="#ink1r" change="#StBa">Continent</add></subst>. <del facs="#img_163_z031" rend="erasure" hand="#HM" change="#StBb"><add facs="#img_163_z031" rend="no-caret" place="inline" hand="#HMp" change="#StBb">Insert</add></del>
