  <lb/><del facs="#img_559_z033" rend="single-stroke _HMp" hand="#HM" change="#StG">Accordingly,</del> <del facs="#img_559_z034" rend="single-stroke _HMp" hand="#HM" change="#StG">he</del> <subst facs="#img_559_z037"><del facs="#img_559_z037" rend="overwritten _HMp" type="superimposed" hand="#HM">in</del><add facs="#img_559_z037" rend="superimposed _HMp" type="superimposed" hand="#HM" change="#StG">In</add></subst> <restore facs="#img_559_z039" change="#StG"><del facs="#img_559_z039" hand="#HM"><hi rend="restoration">obedience to Captain Vere,</hi></del></restore>
